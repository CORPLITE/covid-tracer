{
  "name": "aws-amplify-vue-sample",
  "version": "1.0.0",
  "private": true,
  "description": "aws-amplify VueJs sample",
  "author": "Amazon Web Services",
  "scripts": {
    "build": "node build/build.js",
    "lint": "vue-cli-service lint",
    "dev": "webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "start": "NODE_ENV=development npm run dev",
    "test": "vue-cli-service test:unit",
    "deploy:dev": "amplify env checkout dev; export NODE_ENV=production'; amplify publish",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@aws-amplify/api": "^3.2.7",
    "@aws-amplify/auth": "^3.3.1",
    "@aws-amplify/core": "^3.4.2",
    "@aws-amplify/ui-vue": "^0.2.5-unstable.25",
    "@mdi/font": "^5.3.45",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "aws-amplify": "^3.0.20",
    "aws-sdk": "^2.209.0",
    "buefy": "^0.8.20",
    "bulmaswatch": "^0.8.1",
    "canvas2svg": "^1.0.16",
    "e": "0.0.4",
    "fontfaceobserver": "^2.1.0",
    "fsts": "^0.0.39",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.2",
